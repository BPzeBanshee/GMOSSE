        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/**********************STAGE COMPLETE HANDLING*************************/
if stage_time &gt; 7650 
&amp;&amp; !instance_exists(obj_boss)
&amp;&amp; !instance_exists(obj_boss_dead)
&amp;&amp; !instance_exists(obj_boss1_omake)
&amp;&amp; !instance_exists(obj_fade)
then completedelay += 1;

if completedelay = 120
    {
    instance_create(x,y,obj_ctrl_clear);
    with obj_rock instance_destroy();
    }

// **********************STAGE TIMELINE HANDLING*************************
// This is the code that controls what happens in the stage, whether it be
// enemies, background effects, timing of music or boss initialisation.

stage_time += 1;

switch stage_time
    {
    case 20:
        {
        scr_playsnd(snd_voice1,0);
        stage_time = 5100;
        break;
        }
    case 6000:
        {
        scr_fademusic(global.music_volume,0.005);
        break;
        }
    case 6200:
        {
        instance_create(xview,yview,obj_warning);
        break;
        }
    case 6550:
        {
        en1 = instance_create(80,-30,obj_en1);
        en1.vspeed = 1;
        with obj_ctrl_music event_user(1); // boss music
        with obj_rock dead = 1;
        bkg_timer = 1;
        break;
        }
    case 6610:
        {
        en2 = instance_create(160,-30,obj_en1);
        en2.vspeed = 1;
        break;
        }
    case 6670:
        {
        en3 = instance_create(240,-30,obj_en1);
        en3.vspeed = 1;
        break;
        }
    case 6790:
        {
        sideenemy = instance_create(-40,40,obj_en2);
        sideenemy.hspeed = 1.5;
        sideenemy2 = instance_create(360,40,obj_en2);
        sideenemy2.hspeed = -1.5;
        break;
        }
    case 6890:
        {
        instance_create(160,-32,obj_en_carrier);
        break;
        }
    case 7100:
        {
        instance_create(160,340,obj_boss_trans);
        instance_create(0,340,obj_orb_trans);
        instance_create(320,340,obj_orb2_trans);
        break;
        }
    case 7550:
        {
        instance_create(160,-32,obj_boss);
        break;
        }
    }
    
// CODE FOR ASTEROIDS
if (stage_time &gt; 2650 &amp;&amp; stage_time &lt; 3200) 
|| (stage_time &gt; 5400 &amp;&amp; stage_time &lt; 6500)
    {
    atk_timer += 1;
    if atk_timer = 30 then instance_create(120,-32,obj_rock_atk);
    if atk_timer = 60 then instance_create(200,-32,obj_rock_atk);
    if atk_timer = 90 then instance_create(40,-32,obj_rock_atk);
    if atk_timer = 120
        {
        instance_create(280,-32,obj_rock_atk);
        atk_timer = 0;
        }
    }
    
// ****** AESTHETICS ******
if stage_time &gt; 6550 &amp;&amp; !instance_exists(obj_boss1_omake)
    {
    if background_vspeed[0]&lt; 4 then background_vspeed[0]+= 0.1;
    }

// Reduced Size Asteroids in background
if bkg_timer &gt; -1 &amp;&amp; count &lt; 8 then bkg_timer -= 0.2;
if bkg_timer = 0
    {
    count += 1;
    bkg_timer = 1;
    rx = random_range(2,318);
    rs = random_range(5,15);
    rd = random_range(240,300);
    r = scr_basicshot(rx,-16,obj_rock,rs,rd);
    r.z = random(100);
    }
</string>
