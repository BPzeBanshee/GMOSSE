            <string>/* COMPULSORY */
global.level = -1;
stage_time = 0;
completedelay = 0; // stage complete handling
global.omake_enabled = 1;

/* VARIABLE HANDLING */
atk_timer = 0;
bkg_timer = -1;
count = 0;

/* BACKGROUND HANDLING */
scr_camera_init(320,0,0);
background_vspeed[0] = 0.1; // sets background scrolling speed
planet1 = instance_create(200,134,obj_planet1);
planet1.image_angle = 46;
</string>
