        instance_create(160,80,obj_3Den3);
        instance_create(180,60,obj_3Den3);
        instance_create(200,40,obj_3Den3);
        speed = 0;
        direction = 270;
        break;
        }
    case 3700:
        {
        jumper1 = instance_create(xview-32,yview+60,obj_3Den1_spawn);
        jumper2 = instance_create(xview+272,yview+60,obj_3Den1_spawn);
        jumper1.direction = 0;
        jumper2.direction = 180;
        en4_spawn1 = 0; en4_spawn2 = 0;
        break;
        }
    case 3900: case 3950: 
        {
        if !instance_exists(jumper1) &amp;&amp; !en4_spawn1
            {
            en4a = instance_create(-32,60,obj_en2);
            en4a.hspeed = 2;
            en4_spawn1 = 1;
            }
        if !instance_exists(jumper2) &amp;&amp; !en4_spawn2
            {
            en4b = instance_create(342,60,obj_en2);
            en4b.hspeed = -2;
            en4_spawn2 = 1;
