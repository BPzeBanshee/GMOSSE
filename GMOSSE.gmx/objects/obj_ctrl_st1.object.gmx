            <string>/**********************STAGE COMPLETE HANDLING*************************/
if stage_time &gt; 7650 
&amp;&amp; !instance_exists(obj_boss)
&amp;&amp; !instance_exists(obj_boss_dead)
&amp;&amp; !instance_exists(obj_boss1_omake)
&amp;&amp; !instance_exists(obj_fade)
then completedelay += 1;

if completedelay = 120
    {
    instance_create(x,y,obj_ctrl_clear);
    with obj_rock instance_destroy();
    }

/**********************STAGE TIMELINE HANDLING*************************/
// This is the code that controls what happens in the stage, whether it be
// enemies, background effects, timing of music or boss initialisation.

if pop_count &gt;= 26 then global.omake_enabled = 1;

stage_time += 1;
switch stage_time
    {
    case 20:
        {
        scr_playsnd(snd_voice1,0);
        break;
        }
    case 300:
        {
        instance_create(80,16,obj_3Den3);
        instance_create(120,16,obj_3Den3);
        instance_create(160,16,obj_3Den3);
        instance_create(200,16,obj_3Den3);
        instance_create(240,16,obj_3Den3);
        break;
        }
    case 475:
        {
        instance_create(160,-32,obj_en_carrier);
        planet1 = instance_create(200,-320,obj_planet1);
        planet1.vspeed = 0.1;
        break;
        }
    case 650:
        {
        enemy1 = instance_create(160,-32,obj_en1);
        enemy1.vspeed = 1.25;
        break;
        }
    case 700:
        {
        enemy1r2 = instance_create(80,-32,obj_en1);
        enemy1r2.vspeed = 1.25;
        enemy1r3 = instance_create(240,-32,obj_en1);
        enemy1r3.vspeed = 1.25;
        break;
        }
    case 900:
        {
        enemy2 = instance_create(-32,60,obj_en2);
        enemy2.hspeed = 2;
        enemy3 = instance_create(xview+272,yview+60,obj_3Den1_spawn);
        enemy3.direction = 180;
        break;
        }
    case 1100:
        {
        enemy3b = instance_create(342,60,obj_en2);
        enemy3b.hspeed = -2;
        break;
        }
    case 1200: // Jumping jack 3D enemies spawn relative to your view position
        {
        enemy3 = instance_create(xview-32,yview+60,obj_3Den1_spawn);
        enemy3.direction = 0;
        break;
        }
    case 1400:
        {
        instance_create(60,20,obj_3Den3);
        instance_create(80,40,obj_3Den3);
        instance_create(100,60,obj_3Den3);
        instance_create(120,80,obj_3Den3);
        instance_create(200,80,obj_3Den3);
        instance_create(220,60,obj_3Den3);
        instance_create(240,40,obj_3Den3);
        instance_create(260,20,obj_3Den3);
        break;
        }
    case 1600:
        {
        enemy5 = instance_create(160,-32,obj_3Den2_spawn);
        enemy5.speed = 1.6;
        enemy5.direction = 270;
        break;
        }
    case 1650:
        {
        pop1 = instance_create(240,-32,obj_en1);
        pop2 = instance_create(80,-32,obj_en1);
        pop1.vspeed = 1.25;
        pop2.vspeed = 1.25;
        break;
        }
    case 1875:
        {
        pop1 = instance_create(240,-32,obj_en1);
        pop2 = instance_create(80,-32,obj_en1);
        pop1.vspeed = 2.5;
        pop2.vspeed = 2.5;
        break;
