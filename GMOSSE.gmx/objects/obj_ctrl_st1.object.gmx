        //jumper3 = instance_create(xview-32,yview+180,obj_3Den1_spawn);
        //jumper4 = instance_create(xview+272,yview+180,obj_3Den1_spawn);
        //jumper3.direction = 0;
        //jumper4.direction = 180;
        break;
