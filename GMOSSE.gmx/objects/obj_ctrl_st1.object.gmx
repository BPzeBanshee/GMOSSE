    case 4300:
        {
        instance_create(160,-32,obj_en_carrier);
        break;
        }
    case 4450:
        {
        instance_create(50,80,obj_3Den3);
        instance_create(80,60,obj_3Den3);
        instance_create(110,40,obj_3Den3);
        instance_create(140,20,obj_3Den3);
        instance_create(180,20,obj_3Den3);
        instance_create(210,40,obj_3Den3);
        instance_create(240,60,obj_3Den3);
        instance_create(270,80,obj_3Den3);
        break;
        }
    case 4650:
        {
        instance_create(-16,120,obj_en4);
        instance_create(-16,170,obj_en4);
        instance_create(336,120,obj_en4);
        instance_create(336,170,obj_en4);
        break;
