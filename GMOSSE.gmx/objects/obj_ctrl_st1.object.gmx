    case 5000:
        {
        enemy5 = instance_create(160,-32,obj_3Den2_spawn);// was x=120
        enemy5.speed = 1.6;
        enemy5.direction = 270;
        /*enemy5b = instance_create(200,-32,obj_3Den2_spawn);
        enemy5b.speed = 1.6;
        enemy5b.direction = 270;*/
